{
    "name": "certmichelin/openid-connect-php",
    "description": "Bare-bones OpenID Connect client",
    "keywords": ["oidc", "openid", "jwt", "oauth2"],
    "license": "Apache-2.0",
    "authors": [{
        "name": "Maxime Escourbiac",
        "email": "maxime.escourbiac@michelin.com"
    }],
    "require": {
        "php": ">=7.1",
        "phpseclib/phpseclib" : ">=3.0.7",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "roave/security-advisories": "dev-master"
    },
    "archive" : {
        "exclude" : [
            ".*",
            "tests"
        ]
    },
    "suggest": {
        "ext-apcu": "For caching remote server responses"
    },
    "autoload" : {
         "classmap": [ "src/"]
    }
}
